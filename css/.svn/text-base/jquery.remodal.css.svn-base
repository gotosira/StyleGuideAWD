/* Dialog buttons */

.remodal-confirm, .remodal-cancel {
    display: inline-block;
    width: 140px;
    padding: 9px 0;
    margin: 0 0 5px 0;
    font-size: 12pt;
    cursor: pointer;
    text-decoration: none;
    text-align: center;
    -webkit-border-radius: 50px;
    border-radius: 50px;
    background-clip: padding-box;
    -webkit-transition: all 0.2s linear;
    -moz-transition: all 0.2s linear;
    -o-transition: all 0.2s linear;
    transition: all 0.2s linear;
}
.remodal-confirm {
    border: none;
    background: #0094FF;
    color: #ffffff;
    border-radius: 5px;
    float: right !important;
    /*margin-right: 10px;*/
    margin-top: 20px;
    width: 30% !important;
}
.remodal-confirm:hover, .remodal-confirm:active {
    background: #0380db;
    color: #ffffff;
}
.remodal-cancel {
    border: 2px solid #BDBDBD;
    background: #f4f4f4;
    color: #BDBDBD;
    border-radius: 5px;
    float: left !important;
    margin-right: 20px;
    width: 26% !important;
    margin-top: 20px;
}
.remodal-cancel:hover, .remodal-cancel:active {
    background: #BDBDBD;
    color: #f4f4f4;
}
