body {
    -webkit-font-smoothing: antialiased;
    margin: 0 auto;
    min-width: 320px;
    min-height: 100%;
    position: relative;
    font-family: "Open Sans",Arial,Helvetica,sans-serif !important;
    color: #404663;
    background-color: #ededed;
    font-size: 0.75em;
    line-height: 1.3;
    overflow-x: hidden;
}

a:after {
    display: none !important;
}

li.teaser_hotel_item p a:active, a:hover, a {
    color: #494949;
}

a:active, a:hover, a {
    text-decoration: none;
}

a {
    background: none repeat scroll 0 0 transparent;
}

:-moz-any-link {
    cursor: pointer;
}

.head_wrapper .top_menuv8 {
    z-index: 50100 !important;
    position: relative;
}

ul {
    list-style: none outside none;
}

.top_menuv8 {
/*     z-index: 10 !important; */
}

.top_menuv8 > ul > li {
    cursor: pointer;
    /* min-width: 72px; */
    font-size: 1.2em;
}

.top_menuv8 > ul > li > ul {
    display: none;
/*     z-index: 99999999 !important; */
    /* cursor: pointer; */
}

.top_menuv8 > ul > li > ul > li > a {
    color: #fff !important;
}

.top_menuv8 {
    display: block;
    padding: 0;
    margin: 0;
    color:#fff;
    text-transform: uppercase;
    font-size: 1.2em;
    position: relative;
}

.top_menuv8 li {
    float: left;
    padding-right: 12px; 
    padding-top: 4px; font-family: Arial, Verdana, tahoma, sans-serif; font-size: 12px; text-transform: initial; color: #BBBBBB;
    /* width: 100px; */
    /* float: left; */
}

ul {
    display: block;
    height: 26px;
    margin: 0;
    list-style: none;
    font-size: 0.8em;
    padding-left: 2%;
}

.top_menuv8 > ul > li:before, .second_arrow:before {
    content: "";
    display: inline-block;
    width: 0;
    height: 0;
    border-top: 4px solid rgba(0,0,0,0);
    border-bottom: 4px solid rgba(0,0,0,0);
    border-left: 4px solid #8B8B8B;
    margin-left: 10px;
    float: right;
    top: 4px;
    position: relative;
    z-index: 3000 !important;
}

.second_arrow:before {
    top: 8px !important;
}

.top_menuv8 li > ul > li:after {
    display: none;
}

.top_menuv8 li:last-child:after {
    content: "";
    display: inline-block;
    width: 0;
    height: 0;
    border:none;
}
.top_menuv8 ul {
    padding-left: 0;
}

.top_menuv8 > ul > li > ul > li {
    float: none;
    background: rgba(0,0,0,0.9);
    padding-top: 5px;
    padding-left: 10px;
    width: 100%;
    margin: 0;
    font-size: 1.25em;
    left: -2px;
    position: relative;
    line-height: 1.8;
    color: #fff;
    padding-bottom: 5px;
    padding-right: 5px;
}

.top_menuv8 > ul > li > ul > li:hover > a, .top_menuv8 > ul > li > ul > li > ul > li:hover > a {
    /* background: rgb(234, 22, 139) !important; */
    color: rgb(234, 22, 139) !important;
    z-index: 3000 !important;
}

a:active, a:hover, a {
    color:#fff;text-decoration: none;
}

.menu_over li:hover > a, .menu_over li a:hover {
    color: rgb(234, 22, 139);
}

a:active {

}

.menu_over > li.hover > .parent:before, .menu_over > li > ul > li.hover > .parent:before,
.menu_over > li > ul > li.hover > .parent:before > li.hover {
    content: '';
    display: inline-block;
    width: 18px;
    font-size: 1em;
    float: left;
    background-image: url('../img/arroww1.svg');
    height: 23px;
    background-repeat: no-repeat;
    background-size: 8px auto;
    margin-top: 0px;
    -moz-transform: scaleX(-1);
    -o-transform: scaleX(-1);
    -webkit-transform: scaleX(-1);
    transform: scaleX(-1);
    filter: FlipH;
    -ms-filter: "FlipH";
    margin-right: 10px;
    z-index: 10000;
    top: 5px;
    position: relative;
    left: -10px;
}

a:after {
    display: none !important;
}

a.parent:after {
    display: inline-block !important;
}

.menu_over > li.hover > a.parent:after, .menu_over > li > ul > li.hover > .parent:after {
    display:none !important;
}

.menu_over > li > a {
    display: block;
}

.menu_over li ul {
    position: absolute;
    left: -9999px;
    font-size: 1em;
}

.menu_over li  ul li {
    clear: both;
}

.menu_over li ul li ul li:before { 
	content: "";
	display: none;
	border:none;
}

.menu_over li  ul li ul {
    float:left;
    padding-left:0;margin-left:0;
}
    
.menu_over li.hover > ul {
    left: -2px;
   
}

.menu_over li.mobile > ul {
    background: #fff !important;
    z-index: 10000 !important;
}

.menu_over li.hover > ul > li.hover > ul {
    padding-top:0;
    min-height: 1000px;
    /* margin-right: 10px; */
    padding-left: 0;
    margin-left: 0;
    margin-left: -5px;
}

.menu_over li li.hover ul {
    left: 100%;
    top: 0;
}

.menu_over li li a {
   display: block;
    background: rgba(0, 0, 0, 0.8);
    position: relative;
    z-index: 100;
}

.menu_over li li li a {
    z-index:200;
}

.menu_over {
    padding-left: 0;
}

.menu_over > li {
    float: none;
    background: #fff;
    text-shadow: none;
    font-family: open sans light;
    font-size: 1.6em;
    border-bottom: 1px solid rgba(0,0,0,0.1);
    padding-right:0;
    z-index: inherit;
}

.menu_over li ul li {
    display: block;
    width:100%;
    background: #fff;
    border-bottom: 1px solid rgba(0,0,0,0.1); 
 }
 
.menu_over li ul li a {
    background: rgb(255, 250, 232);color:#000;
    font-size: 0.85em;
    text-align: left;
    text-shadow: none;
}

.menu_over li ul li ul li.mobile a {
    background:rgb(233,255,253) !important;
    font-size: 0.85em;
    padding-right: 100px;
}

.menu_over li ul li ul li.mobile.active a {
    background:rgb(206,232,230) !important;
    font-size: 0.85em;
}

.menu_over > li.mobile.hover > ul > li.mobile.hover a.parent {
    background: rgb(233,233,233) !important;
    text-align: center;
}

.menu_over > li.active {
    background: rgb(223, 223, 223);
    border-bottom: none;
}

.menu_over > li.active:hover {
    background: rgb(223, 223, 223);
}

.menu_over > li.hover, .menu_over li:hover {
    background:rgb(223,223,223);
}

.menu_over > li > a {
    color: #000 !important;
    text-shadow: none;
    text-shadow: none;
    font-weight: 100;
}

.menu_over > li.mobile.hover > ul > li.mobile.active > a.parent {
    background:rgb(255,243,202);
}

.menu_over > li.mobile.hover > ul > li.mobile.hover.active >  a.parent {
    background:rgb(223,223,223);
    text-align:center;
    padding-right: 100px;
    text-shadow: none;
}
    
.menu_over > li.mobile.hover.active > a.parent {
    background:rgb(223,223,223);
}

.menu_over > li.mobile.hover > ul > li.mobile > a.parent {
	background: rgb(255, 250, 232);
	text-shadow: none;
}
    
.menu_over li li .parent {
    background-repeat: no-repeat;
    background-position: 95% 50%;
}

.menu_over ul {
    display: block;
    width: 100%;
}
   
.menu_over > li.hover > ul , .menu_over li li.hover ul {
    position: absolute;
    left: 0;
    padding: 0;
    margin: 0;
    height: 100%;
    display: block;
    z-index: 9000;
    top: -86px;
}
    
.menu_over li ul {
    font-size: 1em;
    padding-left: 0;
    width: 100%;
}

ul.menu_over.hover, ul.menu_over li ul.menu_over.hover {
  -vendor-animation-duration: 1s;
  -vendor-animation-delay: 1s;
  -vendor-animation-iteration-count: 1;
}

.top_menuv8 > ul > li > ul > li > ul {
    display: none;
    left: 0;
    font-size: 1em;
    float: left;
    padding-left: 20px;
    margin-left: 0;
    display: block;
    position: absolute;
    /* width: 100%; */
    height: auto;
    left: 100%;
    top: 0;
    background: #000;
    /* padding: 10px; */
    padding-top: 0;
    /* padding-left: 10px; */
    min-width: auto;
}

.top_menuv8 > ul > li > ul > li > ul > li > ul > li > a, 
.top_menuv8 > ul > li > ul > li > ul > li > a, 
.top_menuv8 > ul > li > a {
    color: #fff !important;
}

.top_menuv8 > ul > li:nth-child(4):before{
    display: none;
}

.top_menuv8 > ul > li:hover > a, 
.top_menuv8 > ul > li > ul > li > ul > li > ul > li:hover > a {
    color: rgb(234, 22, 139) !important;
}

.top_menuv8 > ul > li > ul > li > ul > li > ul {
	position: absolute;
	left: 100%;
	background: #000;
	height: auto;
	/* width: 10px; */
	top: 0;
	padding: 10px;
	padding-top: 0;
	min-width: 180px;
	display: block;
	float: none;
	float: left;
	display: inline-block;
}

.top_menuv8 > ul > li > ul > li > ul > li {
    width: auto;
    display: block;
    min-width: 120px;
    right: 10px;
    position: relative;
}

.top_menuv8 > ul > li > ul > li > ul > li > ul > li {
    width: 100%;
    padding-right: 0;
    margin: 0;
    display: block;
    float: left;
}

.top_menuv8 ul li ul li:hover:last-child ul { 
	top: 0 !important; 
	left: 100% !important;
}