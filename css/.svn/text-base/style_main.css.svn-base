        border-radius: 2px !important;
        box-shadow: none !important;
        background: #2B92DC !important;
        border-style: none !important;
        color: #fff !important;
    }
    table button:active, table input[type="button"]:active, table input[type="reset"]:active, table input[type="submit"]:active {
    background: #0380db !important;
    color: #FFFFFF!important;
    }

