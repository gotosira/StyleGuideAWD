/* Switch Tab */
.function-bar-gray {
	background-color: #CCCCCC;
	height: 30px;
	width: 740px;
	margin-right: auto;
	margin-left: auto;
	margin-bottom: 20px;
	padding: 5px;
}

.tab-function-bar-gray {
	background-color: #CCCCCC;
	height: 8px;
	width: 100%;
	margin-right: auto;
	margin-left: auto;
	margin-bottom: 1px;
	position: relative;
	margin-top: 48px;
}
.tab-first-active {
	float: left;
	height: 28px;
	width: 112px;
	background-color: #CCCCCC;
	display: block;
	position: absolute;
	top: -28px;
	text-transform: uppercase;
	color: #666666;
	text-align: center;
	line-height: 2.3em;
	text-shadow: #ececec 1px 1px;
	font-size: 14px;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}
.tab-first-inactive {
	float: left;
	height: 28px;
	width: 112px;
	background-color: #ffffff;
	display: block;
	position: absolute;
	top: -28px;
	text-transform: uppercase;
	color: #CCCCCC;
	text-align: center;
	line-height: 2.3em;
	text-shadow: #ffffff 1px 1px;
	font-size: 14px;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}
.tab-first-active a,
.tab-first-active a:hover  {
	color: #494949;
	text-decoration: none;
	display: block;
}
.tab-first-inactive a {
	color: #CCCCCC;
	text-decoration: none;
	display: block;
}
.tab-first-inactive a:hover {
	color: #999999;
	text-decoration: none;
	display: block;
}
.tab-second-active {
	float: left;
	height: 28px;
	width: 112px;
	background-color: #CCCCCC;
	display: block;
	position: absolute;
	top: -28px;
	text-transform: uppercase;
	color: #666666;
	text-align: center;
	line-height: 2.3em;
	text-shadow: #ececec 1px 1px;
	font-size: 14px;
	left: 115px;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}
.tab-second-inactive {
	float: left;
