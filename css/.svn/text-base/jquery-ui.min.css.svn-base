.ui-menu {
    list-style: none;
    padding: 0;
    margin: 0;
    display: block;
    outline: none;
    background: #fff;
    box-shadow: 0 1px 1px #ccc;
}
.ui-menu .ui-menu {
    position: absolute;
}
.ui-menu .ui-menu-item {
    position: relative;
	margin: 0;
	padding: 3px 1em 3px .4em;
	cursor: pointer;
	min-height: 0;
	background: #fff;
	font-size: 14px;
	padding-top: 5px;
	padding-bottom: 5px;
	line-height: 1.8;
	height: 100%;


}
.ui-menu-item a {
	padding-left: 10px;
}
.ui-menu .ui-menu-item:hover{
	background: #E4E4E4;
	font-size: 14px;
	color:;
	
}
.ui-menu .ui-menu-divider {
    margin: 5px 0;
    height: 0;
    font-size: 0;
    line-height: 0;
    border-width: 1px 0 0 0;
}
.ui-menu .ui-state-focus, .ui-menu .ui-state-active {
   background: #E4E4E4;
   width:100%;
}
.ui-menu-icons {
    position: relative;
}
.ui-menu-icons .ui-menu-item {
    padding-left: 2em;
}
.ui-menu .ui-icon {
    position: absolute;
    top: 0;
    bottom: 0;
    left: .2em;
    margin: auto 0;
}
.ui-menu .ui-menu-icon {
    left: auto;
    right: 0;
}
.ui-autocomplete-category {
	font-weight: bold;
	padding: .2em .4em;
	margin: .8em 0 0;
	line-height: 1.5;
	background: #facf33;
	margin-top: 0;
	font-weight: 600;
	color: #000;
	font-size: 14px;
	text-transform: uppercase;
	height: 30px;
	padding-top: 10px;
	padding-left: 20px;
	border: 0;
	border-top-left-radius: 0;
	border-top-right-radius: 0;
	height: 100%;
}
